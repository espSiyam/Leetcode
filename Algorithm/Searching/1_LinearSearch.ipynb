    "def LinearSearch(arr, find):\n",
    "    for i , num in enumerate(arr):\n",
    "        if num == find:\n",
    "            return i\n",
    "        \n",
    "\n",
    "arr = [10, 20, 80, 30, 60, 50, 110, 100, 130, 170]\n",
    "find = 130\n",
    "position = LinearSearch(arr, find)\n",
    "print(position)"
